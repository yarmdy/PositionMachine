<local:ZHWindow x:Class="ZEHOU.PM.Label.Camera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZEHOU.PM.Label"
        mc:Ignorable="d"
        Title="摄像头" Height="376" Width="577" WindowStartupLocation="CenterScreen" Closed="ZHWindow_Closed">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top">
            <Button DockPanel.Dock="Left" Click="Button_Click">窗口固定</Button>
            <ComboBox Name="cbCamera" SelectionChanged="cbCamera_SelectionChanged"></ComboBox>
        </DockPanel>
        <Image Name="imgCamera"></Image>
    </DockPanel>
</local:ZHWindow>
